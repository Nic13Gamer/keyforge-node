{
  "name": "@keyforge/client",
  "version": "1.0.0",
  "description": "Keyforge SDK for client applications",
  "scripts": {
    "build": "tsdown",
    "lint": "tsc"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./token": {
      "import": "./dist/token/index.mjs",
      "types": "./dist/token/index.d.ts",
      "default": "./dist/token/index.js"
    }
  },
  "devDependencies": {
    "@types/node": "^22.16.0",
    "tsdown": "^0.12.9",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "jose": "^6.0.11"
  },
  "keywords": [
    "software license",
    "license key",
    "offline license",
    "license management",
    "license key token"
  ],
  "license": "MIT",
  "homepage": "https://keyforge.dev",
  "author": "Keyforge",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Nic13Gamer/keyforge-node.git"
  }
}
